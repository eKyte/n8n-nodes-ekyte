{
  "name": "@ekyte/n8n-nodes-ekyte",
  "version": "1.0.3",
  "description": "Custom n8n nodes for eKyte API integration",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes",
    "n8n",
    "n8n-nodes-ekyte",
    "ekyte"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ekyte/n8n-nodes-ekyte#readme",
  "author": {
    "name": "eKyte Team",
    "email": "dev@ekyte.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ekyte/n8n-nodes-ekyte.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EKyteApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EKyte/EKyteAction.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "gulp-cli": "^2.3.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@ekyte/n8n-nodes-ekyte": "file:ekyte-n8n-nodes-ekyte-1.0.2.tgz"
  }
}
